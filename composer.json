{
    "name": "iam-adty/php-component-adminlte",
    "description": "AdminLTE implementation with PHP Component",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagus A Priambada",
            "email": "iam.adty@gmail.com"
        }
    ],
    "require": {
        "iam-adty/php-component-page": "dev-develop",
        "iam-adty/php-component-html": "dev-develop",
        "slowprog/composer-copy-file": "~0.3",
        "almasaeed2010/adminlte": "~3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "IamAdty\\Component\\": "src/"
        }
    },
    "require-dev": {
        "kint-php/kint": "^3.3"
    },
    "scripts": {
        "post-install-cmd": [
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ],
        "post-update-cmd": [
            "@composer dump-autoload",
            "SlowProg\\CopyFile\\ScriptHandler::copy"
        ]
    },
    "extra": {
        "copy-file": {
            "vendor/almasaeed2010/adminlte/dist/": "public/assets/vendor/adminlte/",
            "vendor/almasaeed2010/adminlte/plugins/": "public/assets/vendor/adminlte/plugins/"
        }
    }
}
